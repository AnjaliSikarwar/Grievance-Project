<%- include('../layouts/header.ejs') %>
  <%- include('../layouts/sidebar.ejs') %>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">

      <!-- Main content -->
      <div class="content">
        <h1>Display Course</h1>

        <!-- /.card -->
        <div class="card">
          <div class="card-header">
            <h3 class="card-title"></h3>
           
          <!-- /.card-header -->
          <div class="card-body">
              
            <table id="example1" class="table table-bordered table-striped">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Course Name</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                <% s.forEach((item)=>{ %>
                  <tr>
                    <td><%= item._id %></td>  
                    <td><%= item.cname %></td>
                   
                   
                    <td>
                      <a href="/admin/courseview/<%= item._id %>" class="btn btn-info">View</a>
                      <a href="/admin/coursedelete/<%= item._id %>" class="btn btn-danger">Delete</a>
                    
                    </td>
                  </tr>
               <% }) %>
               
              </tbody>
              <tfoot>
                <tr>
                    <th>Id</th>
                    <th>Course Name</th>
                    <th>Action</th>
                    
                </tr>
              </tfoot>
            </table>
          </div>
          <!-- /.card-body -->
        </div>
        <!-- /.card -->


      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <%- include('../layouts/footer.ejs') %>